{
  "name": "reg-file-parser",
  "version": "0.0.1",
  "description": "A library for parsing Windows Registry export files (.reg).",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Robert Cato <saiwolf@swmnu.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn run tsc",
    "format": "yarn run prettier --write \"src/**/*.ts\"",
    "lint": "yarn run eslint ./src --ext .ts",
    "prepare": "yarn run build",
    "prepublishOnly": "npm-run-all lint format",
    "preversion": "npm-run-all lint format",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiwolf/reg-file-parser"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "registry",
    "windows-registry",
    "registry-file"
  ],
  "bugs": {
    "url": "https://github.com/saiwolf/reg-file-parser/issues"
  },
  "homepage": "https://github.com/saiwolf/reg-file-parser#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
